{% set probability = 0 %}

{% if score < 450 %}
    {% if age < 18 %}
        .1
    {% elseif score < 40 %}
        1
    {% else %}
        .5
    {% endif %}
{% elseif score < 650 %}
    {% if age < 18 %}
        .2
    {% elseif score < 40 %}
        2
    {% else %}
        2.5
    {% endif %}
